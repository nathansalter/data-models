        "unitTaxSpecification",
        "additionalProperty",
        "accessCode",
        "accessToken",
        "error",
        "cancellationMessage",
        "customerNotice",
        "orderItemIntakeForm"
      ],
      "requiredOptions": [

      ]
    },
    "Cresponse": {
      "hasId": true,
      "requiredFields": [
        "type",
        "acceptedOffer",
        "orderedItem"
      ],
      "recommendedFields": [
        "allowCustomerCancellationFullRefund"
      ],
      "shallNotInclude": [
        "orderItemStatus",
        "cancellationMessage",
        "customerNotice",
        "accessCode",
        "accessToken"
      ],
      "requiredOptions": [

      ]
    },
    "response": {
      "hasId": true,
      "requiredFields": [
        "type",
        "id",
        "orderItemStatus",
        "acceptedOffer",
        "orderedItem"
      ],
      "recommendedFields": [
        "allowCustomerCancellationFullRefund",
        "accessCode",
        "accessToken"
      ],
      "shallNotInclude": [
        "cancellationMessage",
        "customerNotice"
      ],
      "requiredOptions": [

      ]
    },
    "ordersFeed": {
      "hasId": true,
      "requiredFields": [
        "type",
        "id",
        "orderItemStatus",
        "acceptedOffer",
        "orderedItem"
      ],
      "recommendedFields": [
        "allowCustomerCancellationFullRefund",
        "accessCode",
        "accessToken"
      ],
      "shallNotInclude": [
        "attendee",
        "attendeeDetailsRequired",
        "orderItemIntakeForm",
        "orderItemIntakeFormResponse"
      ],
      "requiredOptions": [

      ]
    },
    "cancellation": {
      "hasId": true,
      "requiredFields": [
        "type",
        "id",
        "orderItemStatus"
      ],
      "recommendedFields": [
      ],
      "shallNotInclude": [
        "allowCustomerCancellationFullRefund",
        "unitTaxSpecification",
