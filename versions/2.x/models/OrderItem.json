{
  "type": "OrderItem",
  "derivedFrom": "https://schema.org/OrderItem",
  "idFormat": "https://schema.org/URL",
  "sampleId": "https://example.com/OrderItem/",
  "validationMode": {
    "C1Request": "request",
    "C1Response": "Cresponse",
