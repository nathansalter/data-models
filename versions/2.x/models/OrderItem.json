    "BRequest": "request",
    "BResponse": "response",
    "OrderProposalPatch": "request",
    "OrderPatch": "cancellation",
    "OrdersFeed": "ordersFeed",
    "OrderStatus": "response"
  },
  "imperativeConfiguration": {
    "request": {
      "hasId": true,
      "requiredFields": [
        "type",
        "acceptedOffer",
        "orderedItem"
      ],
      "recommendedFields": [

      ],
      "shallNotInclude": [
        "orderItemStatus",
